<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Scrolling Nav - Start Bootstrap Template</title>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />

    <!-- Bootstrap core CSS -->

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />

    <!-- Custom styles for this template -->
    <link href="~/Content/scrolling-nav.css" rel="stylesheet" />
    <!-- CSS only -->
    <link href="~/Content/style.css" rel="stylesheet" />
    <link href="~/Content/Media.css" rel="stylesheet" />
</head>

<body id="page-top">

    <!-- Navigation -->
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top" id="mainNav">
        <div class="container">
            <!-- <a class="navbar-brand js-scroll-trigger" href="#page-top">Start Bootstrap</a>-->
            
            <img src="~/project image/logo11.png" width="110px" height="60px" />
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarResponsive">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item" c  >
                        <a class="nav-link js-scroll-trigger"  id="headnav" href="/Home/Index"  style="padding-right:30px;">Home</a>
                    </li>
                    <li class="nav-item" class="navb">
                        
                        <a class="nav-link js-scroll-trigger" id="headnav" href="/Home/AboutUs" style="padding-right:30px; ">About Us</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link js-scroll-trigger" id="headnav" href="/Home/ContactUs" style="padding-right:30px; ">Contact Us</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link js-scroll-trigger" id="headnav" href="/Home/Events" style="padding-right:30px; ">Events</a>
                    </li>

                    <li class="nav-item">
                        @if (Session["log"] == "Log out")
                        {
                            <a href="/Home/Logout"> <button class="btn btn-outline-danger" type="submit">Log Out</button></a>}
                        else
                        {
                            <a href="/Home/Login"> <button class="btn btn-outline-danger" type="submit">Log In</button></a>}

                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <div>

        @RenderBody()

    </div>


    <!-- Footer -->
            <footer class="py-4 bg-dark">
                <div class="container">
                    <p class="m-0 text-center text-white">Copyright &copy; Your Website 2019</p>
                </div>
                <!-- /.container -->
            </footer>

            <!-- Bootstrap core JavaScript -->

            <script src="~/Scripts/jquery-3.5.1.min.js"></script>


            <script src="~/Scripts/bootstrap.bundle.min.js"></script>

            <!-- Plugin JavaScript -->

            <script src="~/Scripts/jquery.easing.1.3.js"></script>

            <!-- Custom JavaScript for this theme -->

            <script src="~/Scripts/scrolling-nav.js">
           <script src="~/Scripts/wow.min.js"></script>
            </script>

            @RenderSection("scripts", false)


</body>

</html>
